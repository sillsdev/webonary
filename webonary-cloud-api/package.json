{
  "name": "webonary-cloud-api",
  "version": "0.1.0",
  "bin": {
    "webonary-cloud-api": "bin/webonary-cloud-api.js"
  },
  "scripts": {
    "apidoc": "apidoc -f '^[^.]+\\.ts$' -i lambda/ -e lambda/__tests__ -e lambda/node_modules  -o lambda_api_doc/",
    "build": "npm run lint && npx tsc",
    "cdk": "env-cmd npx aws-cdk@2.x",
    "deploy": "npm run test && npm run cdk deploy",
    "deploy-local": "npm run env-cmd cdk synth --no-staging >| template.yaml && sam local start-api",
    "lint": "tsc --noEmit && eslint '*/**/*.ts' --fix",
    "lint-dry-run": "tsc --noEmit && eslint '*/**/*.ts' --fix-dry-run",
    "migrate": "cd mongo_utils && env-cmd -f ../.env mongo-migrate",
    "post-legacy": "env-cmd ts-node -r tsconfig-paths/register tools/post-legacy.ts",
    "test": "tsc && jest --detectOpenHandles --runInBand --force-exit",
    "test-coverage": "tsc && jest --detectOpenHandles --runInBand --force-exit --collectCoverage",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.32.0",
    "@types/cheerio": "^0.22.31",
    "@types/jest": "^28.1.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.0.0",
    "@types/parse5": "^6.0.3",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "aws-cdk-lib": "^2.186.0",
    "constructs": "^10.0.0",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.14.49",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.0.0",
    "form-data": "^4.0.0",
    "fs": "0.0.1-security",
    "jest": "^28.1.1",
    "parse5": "^7.0.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.186.0",
    "axios": "^1.6",
    "cheerio": "^1.0.0-rc.12",
    "constructs": "^10.0.0",
    "mime-types": "^2.1.35",
    "mongo-migrate-ts": "^1.1.1",
    "mongodb": "^4.17.1",
    "npm-check-updates": "^16.10.7"
  }
}
